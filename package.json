{
  "name": "discord-gamestatus",
  "version": "2.0.0",
  "description": "Monitor game servers via discord",
  "main": "dist/index",
  "directories": {
    "lib": "src",
    "bin": "bin"
  },
  "author": "Douile",
  "keywords": [
    "discord",
    "discord-bot",
    "discord-gamestatus",
    "gamestatus",
    "monitoring",
    "game-server-monitoring"
  ],
  "scripts": {
    "test": "node bin/run-tests.js",
    "test:lint": "npx eslint src --ext .ts,.js",
    "test:format": "npx prettier --check ./bin ./src",
    "build": "npx tsc",
    "format": "npx prettier --write ./bin ./src",
    "start": "npm run build && node ./bin/discord-gamestatus"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Douile/discord-gamestatus/issues"
  },
  "homepage": "https://github.com/Douile/discord-gamestatus",
  "repository": {
    "type": "git",
    "url": "https://github.com/Douile/discord-gamestatus.git"
  },
  "engines": {
    "node": ">=17.0.0"
  },
  "dependencies": {
    "@douile/bot-utilities": "^1.4",
    "discord.js-light": "^4.6",
    "gamedig": "^4.0",
    "node-fetch": "^2.6",
    "pg": "^8.7"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0",
    "erlpack": "github:discord/erlpack",
    "utf-8-validate": "^5.0",
    "zlib-sync": "^0.1"
  },
  "devDependencies": {
    "@types/gamedig": "^3.0",
    "@types/pg": "^8.6",
    "@types/ws": "^8.5",
    "@typescript-eslint/eslint-plugin": "^5.17",
    "@typescript-eslint/parser": "^5.17",
    "eslint": "^8.12",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6",
    "typescript": "^4.6"
  }
}
